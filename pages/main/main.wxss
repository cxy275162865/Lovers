.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}

.scroll_view {
	width: 95%;
	height: 600rpx;
	position: fixed;
	bottom: 20rpx;
}

.view{
	display: flex;
	flex-direction: column;
	align-items: center;
}

.text{
	margin-top: 20rpx;
	margin-left: 15rpx;
	margin-right: 15rpx;
	color: rgb(196, 189, 189);
	font-size: 40rpx;
}

.redline{
	padding-top: 150rpx;
	font-size: 50rpx;
	color: rgb(175, 59, 59);
}

.redline2{
	font-weight: bold;
	color: rgb(206, 64, 64);
	margin-top: 20rpx;
	font-size: 30rpx;
}

#heart{
	position: fixed;
	right: 40rpx;
	top: 120rpx;
	height: 150rpx;
	width: 150rpx;
	opacity: 0.7;
}

#cxy {
	position: fixed;
	left: 30rpx;
	top: 450rpx;
	height: 200rpx;
	width: 200rpx;
	opacity: 0.8;
}

#wbx {
	position: fixed;
	right: 20rpx;
	top: 480rpx;
	height: 150rpx;
	width: 150rpx;
	opacity: 0;
}

#bg {
	position: fixed;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	z-index: -1;
}

